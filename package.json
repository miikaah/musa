{
  "name": "musa",
  "version": "3.0.1",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "env-cmd -f .env.server.local craco start",
    "start:electron": "env-cmd -f .env.electron.local craco start",
    "build": "env-cmd -f .env.server.prod react-scripts build",
    "build:electron": "env-cmd -f .env.electron.prod craco build && git rev-parse HEAD > version",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "lodash.defaultto": "^4.14.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.sortby": "^4.7.0",
    "quantize": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "styled-breakpoints": "^12.1.2",
    "styled-components": "^6.0.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "craco-esbuild": "^0.5.2",
    "env-cmd": "^10.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0"
  }
}
