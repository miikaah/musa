{
  "name": "musa",
  "version": "3.0.1",
  "private": true,
  "homepage": "./",
  "type": "module",
  "scripts": {
    "setup": "shx mkdir build && shx mkdir build-server && shx cp .env.electron.ci.example .env.electron.prod && shx cp .env.server.ci.example .env.server.prod",
    "start:s": "env-cmd -f .env.server.local vite --port 3667 --mode server",
    "start:e": "env-cmd -f .env.electron.local vite --port 3666 --mode electron",
    "build:s": "shx rm -rf build-server && shx mkdir build-server && shx cp -R public/* build-server && env-cmd -f .env.server.prod node scripts/build.mjs && git rev-parse HEAD > version",
    "build:e": "shx rm -rf build && shx mkdir build && shx cp -R public/* build && env-cmd -f .env.electron.prod node scripts/build.mjs && git rev-parse HEAD > version",
    "tsc": "tsc --noEmit",
    "tsc:w": "tsc --watch --noEmit",
    "test": "vitest run --coverage",
    "test:w": "vitest",
    "lint": "eslint src"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.3.1",
    "@fortawesome/fontawesome-svg-core": "6.7.1",
    "@fortawesome/free-solid-svg-icons": "6.7.1",
    "@fortawesome/react-fontawesome": "0.2.2",
    "lodash.isequal": "4.5.0",
    "lodash.sortby": "4.7.0",
    "quantize": "1.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "8.1.3",
    "react-router": "6.28.0",
    "react-router-dom": "6.28.0",
    "redux": "4.2.1",
    "styled-breakpoints": "14.1.4",
    "styled-components": "6.1.13"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@miikaah/musa-core": "10.4.8",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/lodash.isequal": "4.5.8",
    "@types/lodash.sortby": "4.7.9",
    "@types/node": "22.10.1",
    "@types/quantize": "1.0.2",
    "@types/react": "18.3.14",
    "@types/react-dom": "18.3.2",
    "@types/react-redux": "7.1.34",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "@vitejs/plugin-react-swc": "3.7.2",
    "@vitest/coverage-v8": "2.1.8",
    "env-cmd": "10.1.0",
    "esbuild": "0.24.0",
    "esbuild-plugin-inline-image": "0.0.9",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "@vitest/eslint-plugin": "1.1.14",
    "handlebars": "4.7.8",
    "jsdom": "25.0.1",
    "prettier": "3.3.3",
    "shx": "0.3.4",
    "typescript": "5.7.2",
    "vite": "5.4.11",
    "vitest": "2.1.8",
    "vitest-dom": "0.1.1"
  }
}
