{
  "name": "musa",
  "version": "3.0.1",
  "private": true,
  "homepage": "./",
  "scripts": {
    "setup": "shx mkdir build && shx mkdir build-server && shx cp .env.electron.ci.example .env.electron.prod && shx cp .env.server.ci.example .env.server.prod",
    "start:s": "vite --port 3667 --mode server",
    "start:e": "vite --port 3666 --mode electron",
    "build:s": "shx rm -rf build-server && shx mkdir build-server && shx cp -R public/* build-server && env-cmd -f .env.server.prod node scripts/build.mjs && git rev-parse HEAD > version",
    "build:e": "shx rm -rf build && shx mkdir build && shx cp -R public/* build && env-cmd -f .env.electron.prod node scripts/build.mjs && git rev-parse HEAD > version",
    "tsc": "tsc --noEmit",
    "tsc:w": "tsc --watch --noEmit",
    "test": "vitest run --coverage",
    "test:w": "vitest"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.2",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.1",
    "lodash.defaultto": "^4.14.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.sortby": "^4.7.0",
    "quantize": "^1.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^8.1.3",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "redux": "^4.2.1",
    "styled-breakpoints": "^14.1.3",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@miikaah/musa-core": "^10.0.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash.isempty": "^4.4.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.sortby": "^4.7.9",
    "@types/node": "^20.12.12",
    "@types/quantize": "^1.0.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.33",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^0.34.6",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.21.3",
    "esbuild-plugin-inline-image": "^0.0.9",
    "eslint": "8.50.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "handlebars": "^4.7.8",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "shx": "^0.3.4",
    "vite": "^4.5.3",
    "vitest": "^0.34.6",
    "vitest-dom": "^0.1.1"
  }
}
